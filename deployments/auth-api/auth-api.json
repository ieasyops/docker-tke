{
  "secure_serving": {
    "tls_cert_file": "/etc/certificates/localhost+8.pem",
    "tls_private_key_file": "/etc/certificates/localhost+8-key.pem"
  },
  "authentication": {
    "token_auth_file": "/etc/token.csv",
    "privileged_username": "admin"
  },
  "generic": {
    "external_hostname": "tkeauthapi",
    "external_port": 9451
  },
